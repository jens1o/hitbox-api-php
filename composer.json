{
    "name": "jens1o/smashcast-api-php",
    "description": "Handles api requests to smashcast nicely!",
    "version": "0.8.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jens Hausdorf",
            "email": "jenshausdorf@gmail.com",
            "role": "Developer / Maintainer",
            "homepage": "https://jens-hausdorf.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": ">=6.2.3",
        "jens1o/php-utils": "^0.2"
    },
    "autoload": {
        "psr-4": {
            "jens1o\\smashcast\\" : "src/",
            "jens1o\\smashcast\\channel\\": "src/channel/",
            "jens1o\\smashcast\\exception\\": "src/exception/",
            "jens1o\\smashcast\\hashtag\\": "src/hashtag/",
            "jens1o\\smashcast\\media\\": "src/media/",
            "jens1o\\smashcast\\media\\live\\": "src/media/live/",
            "jens1o\\smashcast\\media\\live\\details\\": "src/media/live/details/",
            "jens1o\\smashcast\\model\\": "src/model/",
            "jens1o\\smashcast\\oauth\\": "src/oauth/",
            "jens1o\\smashcast\\token\\": "src/token/",
            "jens1o\\smashcast\\user\\": "src/user/",
            "jens1o\\smashcast\\user\\logo\\": "src/user/logo/",
            "jens1o\\smashcast\\user\\oauth\\": "src/user/oauth/",
            "jens1o\\smashcast\\util\\": "src/util/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload-dev": {
        "psr-4": {
            "jens1o\\smashcast\\test\\": "test/"
        }
    }
}
